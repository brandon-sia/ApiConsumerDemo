<Window x:Class="ApiConsumerDemo.SunInfo"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ApiConsumerDemo"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SunInfo" WindowStartupLocation="CenterScreen">

    <Grid>
 
        <StackPanel>
            <Button x:Name="loadSunInfo" Content="Load Sun Info" Margin="20" Click="loadSunInfo_Click"/>
            <TextBlock x:Name="sunriseText"  Margin="20"/>
            <TextBlock x:Name="sunsetText" Margin="20"/>
        </StackPanel>

    </Grid>
</Window>
